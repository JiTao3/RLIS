{
  "id": "Test_Basketball_Database_00",
  "description": "Just a Basketball Test Experiment",
  "database_name": "basketball",
  "random_seed": 60,
  "max_index_width": 2,
  "reenable_indexes": true,
  "result_path": "exp_result",
  "max_steps_per_episode": 200,
  "rl_algorithm": {
    "algorithm": "PPO",
    "stable_baselines_version": 3,
    "gamma": 0.5,
    "policy": "MultiInputPolicy",
    "model_architecture": {
      "net_arch": [
        {
          "vf": [256,256],
          "pi": [256,256]
        }
      ],
      "features_extractor_class": "FeaturesExtractor",
      "features_extractor_kwargs": {
        "features_dim": 124
      }
    },
    "args": {
      "n_steps": 64
    }
  },
  "workload": {
    "benchmark": "basketball",
    "workload_window_size": 100,
    "tabel_num_row_filter": 10000,
    "scale_factor": 1
  },
  "workload_embedder": {
    "representation_size": 124,
    "max_columns_per_query": 16,
    "max_edges": 100,
    "sql_predicates_vec_size": 64
  }
}
