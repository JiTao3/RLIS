{
  "id": "All_Database_01",
  "description": "Test Experiment for all databases",
  "random_seed": 60,
  "max_index_width": 2,
  "reenable_indexes": true,
  "result_path": "exp_result",
  "max_steps_per_episode": 100,
  "parallel_environments": 1,
  "statistics_path": "exp_statistics",
  "load_pretrained_model": false,
  "port": "12333",
  "cuda_device": "cuda:0",
  "rl_algorithm": {
    "algorithm": "PPO",
    "stable_baselines_version": 3,
    "policy": "MultiInputPolicy",
    "total_timesteps": 20,
    "model_params": {
      "learning_rate": 3e-4,
      "ent_coef": 0.01,
      "args": {
        "gamma": 0.95,
        "batch_size": 128,
        "n_epochs": 5,
        "n_steps": 1024,
        "clip_range": 0.2,
        "vf_coef": 0.5,
        "clip_range_vf": 0.1,
        "target_kl": null,
        "max_grad_norm": 1.0
      },
      "model_architecture": {
        "net_arch": {
          "vf": [
            256,
            512,
            256
          ],
          "pi": [
            256,
            512,
            256
          ]
        },
        "features_extractor_class": "FeaturesExtractor",
        "features_extractor_kwargs": {
          "features_dim": 124
        }
      }
    }
  },
  "workload": {
    "debug_mode": true,
    "workload_window_size": 200,
    "tabel_num_row_filter": 10000,
    "scale_factor": 1,
    "benchmarks": [
      "credit",
      "sakila",
      "stats"
    ],
    "eval_benchmarks": 
    ["stats"]
  },
  "workload_embedder": {
    "representation_size": 124,
    "max_columns_per_query": 32,
    "max_edges": 64,
    "sql_predicates_vec_size": 64,
    "sql_embedding_dim": 228
  }
}